/boot         emmc     boot                     flags=display=boot
/cache        ext4     /dev/block/mmcblk0p7     flags=display=cache
/data         ext4     /dev/block/mmcblk0p8     flags=display=data
/misc         emmc     misc                     flags=display=misc
/recovery     emmc     recovery                 flags=display=recovery
/fac          ext4     /dev/block/mmcblk0p5     flags=display=fac
/system       ext4     /dev/block/mmcblk0p6     flags=display=system
/sdcard       vfat     /dev/block/mmcblk0p9     flags=display=sdcard
